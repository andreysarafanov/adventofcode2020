    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "downlevelIteration": true,
    "lib": ["es2017", "DOM"],
    "strict": true,
    "target": "es5"
  },
  "include": ["src/**/*.ts"]
}
